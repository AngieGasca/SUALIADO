@model Sualiado.Models.Request.ProductRequest

@{
    ViewBag.Title = "Agregar Producto";
}
@using (Html.BeginForm())
{

    <div class="form-group col-md-5">
        @Html.LabelFor(p => p.Cod_Subca)
        @Html.EditorFor(p => p.Cod_Subca, new { htmlAttributes = new { @class = "form-control" } })

    </div>

    <div class="form-group col-md-5">
        @Html.LabelFor(p => p.Nombre_Produc)
        @Html.TextBoxFor(p => p.Nombre_Produc, new { @class = "form-control" })
    </div>

    <div class="form-group col-md-5">
        @Html.LabelFor(p => p.Existencia)
        @Html.EditorFor(p => p.Existencia, new { htmlAttributes = new { @class = "form-control" } })
    </div>

    <div class="form-group col-md-5">

        @Html.LabelFor(p => p.Precio_Vta)
        @Html.EditorFor(p => p.Precio_Vta, new { htmlAttributes = new { @class = "form-control" } })
    </div>


    <div class="form-group col-md-5">
        @Html.LabelFor(p => p.Activo)
        @Html.EditorFor(p => p.Activo, new { htmlAttributes = new { @class = "form-control" } })
    </div>



    <input type="submit" value="Agregar" />


}
