
@model Sualiado.Models.Request.ProviderRequest

@{
    ViewBag.Title = "Editar Proveedor";
}

@using (Html.BeginForm())
{
    @Html.HiddenFor(p => p.Cod_Prove)

    <div class="form-group col-md-5">
        @Html.LabelFor(p => p.Cod_Pers)
        @Html.EditorFor(p => p.Cod_Pers, new { htmlAttributes = new { @class = "form-control" } })

    </div>
    <div class="form-group col-md-5">
        @Html.LabelFor(p => p.Locacion)
        @Html.TextBoxFor(p => p.Locacion, new { @class = "form-control" })
    </div>
    <div class="form-group col-md-5">
        @Html.LabelFor(p => p.Enlace)
        @Html.TextBoxFor(p => p.Enlace, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="form-group col-md-5">
        @Html.LabelFor(p => p.Activo)
        @Html.EditorFor(p => p.Activo, new { htmlAttributes = new { @class = "form-control" } })
    </div>


    <input type="submit" value="Editar" />

}


